    "eslint": "~2.9.0",
    "eslint-config-fatfisz": "~2.9.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^5.0.3",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
